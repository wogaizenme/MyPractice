<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_expandable_list_view_type"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.practice.expandablelistview.type.ExpandableListViewTypeActivity">

    <ExpandableListView
        android:id="@+id/expandable_listview_type"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:childDivider="@drawable/listview_divider"
        android:divider="#00000000"
        android:dividerHeight="1dp"
        android:footerDividersEnabled="true"
        android:groupIndicator="@null"
        android:headerDividersEnabled="true" />
</RelativeLayout>
